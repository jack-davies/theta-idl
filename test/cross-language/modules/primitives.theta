language-version: 1.0.0
avro-version: 1.1.0
---

/// A field for each primitive type:
type Primitives = {
  bool     : Bool,
  bytes    : Bytes,
  int      : Int,
  long     : Long,
  float    : Float,
  double   : Double,
  string   : String,
  date     : Date,
  datetime : Datetime
}

/// Each kind of container:
type Containers = {
    array    : [Bool],
    map      : {Int},
    optional : Long?,
    nested   : {[Date?]}
}
